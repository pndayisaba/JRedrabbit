<div id="post-right-@forum_id" class="post-right">
	<div id="post-title-@forum_id" class="post-title">@title</div>
	<div id="posted-by-@forum_id" class="posted-by">
		<span id="posted-by-@forum_id" class="posted-by">@created_by_name</span>
		<span id="post-date-@forum_id" class="post-date"> - @date</span>
	</div><!--posted-by -->
	<div id="post-desc-@forum_id" class="post-desc">@description</div>
	<div id="take-action-@forum_id" class="take-action">
		<a href="javascript:;" id="reply-@forum_id" class="p-control reply">Reply </a>
		<a href="javascript:;" id="edit-@forum_id" class="p-control edit"> Edit </a>
        <a href="javascript:;" id="delete-@forum_id" class="p-control delete"> Delete </a>
    </div>
    <hr class="end-parent-hr">
    
    
</div>
